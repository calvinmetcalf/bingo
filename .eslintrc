{
  "env": {
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
        "modules": true,
        "experimentalObjectRestSpread": true,
         "jsx": true

    },
  "rules": {
    "indent": [2, 2],
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "no-console": 0,
    "no-shadow": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-underscore-dangle": 0,
    "no-constant-condition": 0,
    "space-after-function-name": 0,
    "no-undef": 2,
    "camelcase": 0  },
  "plugins": [
    "react"
  ],
   "globals": {
        "document": false,
        "XMLHttpRequest": false
    }
}
